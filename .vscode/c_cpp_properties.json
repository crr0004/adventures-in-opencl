{
    "configurations": [
        {
            "name": "MinGW Clang",
            "includePath": [
                "${workspaceFolder}/**",
                "${config:user.mingw-w64-path}/include/**",
                "${config:user.mingw-w64-path}/x86_64-w64-mingw32/include/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.17134.0",
            "compilerPath": "${config:user.mingw-w64-path}/bin/clang++.exe",
            "intelliSenseMode": "clang-x64",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "configurationProvider": "vector-of-bool.cmake-tools",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}