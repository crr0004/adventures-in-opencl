{
    "configurations": [
        {
            "name": "MinGW Clang",
            "includePath": [
                "${workspaceFolder}/**",
                "${config:user.mingw-w64-path}/include/**",
                "${config:user.mingw-w64-path}/x86_64-w64-mingw32/include/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.17134.0",
            "compilerPath": "${config:user.mingw-w64-path}/bin/clang++.exe",
            "intelliSenseMode": "clang-x64",
            "cStandard": "c11",
            "cppStandard": "c++20"
        },
        {
            "name": "Win Clang",
            "cppStandard": "c++20",
            "cStandard": "c11",
            "intelliSenseMode": "msvc-x64",
            "compilerPath": "F:/dev/bin/llvm/bin/clang++.exe",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "compilerArgs": ["-###"]
        },
        {
            "name": "Linux Clang",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "clang-x64"
        }
    ],
    "version": 4
}